local originalTextures = {}

local function removeTextures()
    for _, texture in pairs(game:GetDescendants()) do
        if texture:IsA("Texture") or texture:IsA("Decal") then
            table.insert(originalTextures, {parent = texture.Parent, texture = texture:Clone()})
            texture:Destroy()
        end
    end
end

local function restoreTextures()
    for _, item in pairs(originalTextures) do
        item.texture.Parent = item.parent
    end
    originalTextures = {}
end

local function optimizeSettings()
    local lighting = game:GetService("Lighting")
    lighting.GlobalShadows = false
    lighting.FogEnd = 9e9
    lighting.Brightness = 0
    lighting.OutdoorAmbient = Color3.new(0, 0, 0)
    lighting.TimeOfDay = "14:00:00"
    
    local settings = UserSettings():GetService("UserGameSettings")
    settings.MasterVolume = 0
end

local function resetSettings()
    local lighting = game:GetService("Lighting")
    lighting.GlobalShadows = true
    lighting.FogEnd = 100000
    lighting.Brightness = 1
    lighting.OutdoorAmbient = Color3.new(127/255, 127/255, 127/255)
    lighting.TimeOfDay = "14:00:00"
    
    local settings = UserSettings():GetService("UserGameSettings")
    settings.MasterVolume = 0.5
end

local isBoosterEnabled = false

fps.MouseButton1Click:Connect(function()
    isBoosterEnabled = not isBoosterEnabled
    if isBoosterEnabled then
        removeTextures()
        optimizeSettings()
        fps.Text = "On"
    else
        restoreTextures()
        resetSettings()
        fps.Text = "Off"
    end
end)

