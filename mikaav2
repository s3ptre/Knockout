-- Scripts:

local function UBQN_script() 
	local script = Instance.new('LocalScript', MainFrame)

	local textLabel = script.Parent:WaitForChild("TextLabel")
	
	local player = game.Players.LocalPlayer
	
	textLabel.Text = "Welcome, " .. player.DisplayName
	
	textLabel.Size = UDim2.new(0, 200, 0, 50) 
	textLabel.Position = UDim2.new{-0, 0},{-0.12, 10}
	textLabel.BackgroundColor3 = Color3.new(0, 0, 0) 
	textLabel.TextColor3 = Color3.new(1, 1, 1) 
	textLabel.Font = Enum.Font.SourceSans 
	textLabel.TextSize = 24 
	textLabel.BackgroundTransparency = 0.5
	
end
coroutine.wrap(UBQN_script)()
local function TYNQWGY_script() 
	local script = Instance.new('LocalScript', MainFrame)

	local screenGui = script.Parent
	local imageLabel = screenGui:WaitForChild("ProfilePicture")
	
	local player = game.Players.LocalPlayer
	
	if player and imageLabel then
		imageLabel.Image = "rbxthumb://type=AvatarHeadShot&id=" .. player.UserId .. "&w=180&h=180"
	
		imageLabel.Size = UDim2.new(0, 70, 0, 68) 
		imageLabel.Position = UDim2.new(0.354, 0, -0.06, 0) 
		imageLabel.BackgroundTransparency = 1 
	else
		warn("Player or ImageLabel not found!")
	end
	
end
coroutine.wrap(TYNQWGY_script)()
local function AVTU_script()  
	local script = Instance.new('LocalScript', MiniFrame1)

	
	local frame = script.Parent 
	
	local rainbowColors = {
		Color3.fromRGB(0, 255, 0),      
		Color3.fromRGB(0, 255, 255),     
		Color3.fromRGB(0, 0, 255),      
		Color3.fromRGB(0, 0, 139)         
	}
	
	local transitionTime = 1  
	
	local function transitionColor()
		while true do
			for i = 1, #rainbowColors - 1 do
				local startColor = rainbowColors[i]
				local endColor = rainbowColors[i + 1]
				local steps = 50 
				local stepTime = transitionTime / steps
	
				for step = 1, steps do
					local lerpedColor = startColor:Lerp(endColor, step / steps)
					frame.BackgroundColor3 = lerpedColor
					wait(stepTime)
				end
			end
		end
	end
	
	transitionColor()
	
end
coroutine.wrap(AVTU_script)()
local function ZYCNHA_script()
	local script = Instance.new('LocalScript', TabButton2)

	local Textbox = script.Parent 
	
	local rainbowColors = {
		Color3.fromRGB(255, 20, 147),   
		Color3.fromRGB(128, 0, 128),    
		Color3.fromRGB(255, 0, 0),
		Color3.fromRGB(123, 74, 135)
	}
	
	local transitionTime = 5
	
	local function transitionColor()
		while true do
			for i = 1, #rainbowColors - 1 do
				local startColor = rainbowColors[i]
				local endColor = rainbowColors[i + 1]
				local steps = 50  
				local stepTime = transitionTime / steps
	
				for step = 1, steps do
					local lerpedColor = startColor:Lerp(endColor, step / steps)
					Textbox.BackgroundColor3 = lerpedColor
					wait(stepTime)
				end
			end
		end
	end
	
	transitionColor()
	
end
coroutine.wrap(ZYCNHA_script)()
