Event:FireServer(A_1, A_2)

local w = {}
local x = {}
local c = {}
local userid = game.Players.LocalPlayer.UserId

local function checkUserId()
    if game.Players.LocalPlayer.UserId ~= userid then
        game.Players.LocalPlayer.UserId = userid
    end
end

game:GetService("RunService").Heartbeat:Connect(checkUserId)

for i, w in pairs(w) do
    if game.Players.LocalPlayer.UserId == w then
        game.Players.LocalPlayer:Kick("Your blacklisted for being an annoying hoe, hahaha noob")
    game.Players.LocalPlayer:Destroy()
    end
end

for i, x in pairs(x) do
    if game.Players.LocalPlayer.UserId == x then
        game.Players.LocalPlayer:Kick("2")
    game.Players.LocalPlayer:Destroy()
    end
end

for i, c in pairs(c) do
    if game.Players.LocalPlayer.UserId == c then
        game.Players.LocalPlayer:Kick("1")
    game.Players.LocalPlayer:Destroy()
    end
end


