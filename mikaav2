	local transitionTime = 5 
	
	local function transitionColor()
		while true do
			for i = 1, #rainbowColors - 1 do
				local startColor = rainbowColors[i]
				local endColor = rainbowColors[i + 1]
				local steps = 50 
				local stepTime = transitionTime / steps
	
				for step = 1, steps do
					local lerpedColor = startColor:Lerp(endColor, step / steps)
					TextButton.BackgroundColor3 = lerpedColor
					wait(stepTime)
				end
			end
		end
	end
	
	transitionColor()
	
end
coroutine.wrap(GVGMCFG_script)()
local function YPWZBNH_script()
	local script = Instance.new('LocalScript', MainFrame)

	local MainFrame = script.Parent
	local TabButton1 = MainFrame:WaitForChild("TabButton1")
	local TabButton2 = MainFrame:WaitForChild("TabButton2")
	local TabButton3 = MainFrame:WaitForChild("TabButton3")
	local TabButton4 = MainFrame:WaitForChild("TabButton4")
	local TabButton5 = MainFrame:WaitForChild("TabButton5")
	local MiniFrame1 = MainFrame:WaitForChild("MiniFrame1")
	local MiniFrame2 = MainFrame:WaitForChild("MiniFrame2")
	local MiniFrame3 = MainFrame:WaitForChild("MiniFrame3")
	local MiniFrame4 = MainFrame:WaitForChild("MiniFrame4")
	local MiniFrame5 = MainFrame:WaitForChild("MiniFrame5")
	
	local function showFrame(selectedFrame)
		for _, frame in ipairs(MainFrame:GetChildren()) do
			if frame:IsA("Frame") and frame.Name:match("MiniFrame") then
				frame.Visible = false
			end
		end
		selectedFrame.Visible = true
	end
	
	TabButton1.MouseButton1Click:Connect(function()
		showFrame(MiniFrame1)
	end)
	
	TabButton2.MouseButton1Click:Connect(function()
		showFrame(MiniFrame2)
	end)
	
	TabButton3.MouseButton1Click:Connect(function()
		showFrame(MiniFrame3)
	end)
	
	TabButton4.MouseButton1Click:Connect(function()
		showFrame(MiniFrame4)
	end)
	
	TabButton5.MouseButton1Click:Connect(function()
		showFrame(MiniFrame5)
	end)
	
	showFrame(MiniFrame1)
	
	
end
coroutine.wrap(YPWZBNH_script)()
local function VOPWTO_script()
	local script = Instance.new('LocalScript', MiniFrame2)

	local frame = script.Parent 
	
	local rainbowColors = {
		Color3.fromRGB(255, 20, 147),   
		Color3.fromRGB(128, 0, 128),    
		Color3.fromRGB(255, 0, 0),
		Color3.fromRGB(123, 74, 135)
	}
	
	local transitionTime = 1 
	
	local function transitionColor()
		while true do
			for i = 1, #rainbowColors - 1 do
				local startColor = rainbowColors[i]
				local endColor = rainbowColors[i + 1]
				local steps = 50  
				local stepTime = transitionTime / steps
	
				for step = 1, steps do
					local lerpedColor = startColor:Lerp(endColor, step / steps)
					frame.BackgroundColor3 = lerpedColor
					wait(stepTime)
				end
			end
		end
	end
	
	transitionColor()
	
end
coroutine.wrap(VOPWTO_script)()
local function FDIRS_script()
	local script = Instance.new('LocalScript', TextLabel_4)

	local UserInputService = game:GetService("UserInputService")
	local mainFrame = script.Parent.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	script.Parent.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = mainFrame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	script.Parent.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			update(input)
		end
	end)
	
end
coroutine.wrap(FDIRS_script)()
local function PMYMP_script()
	local script = Instance.new('LocalScript', infom)

	local textLabel = script.Parent
	local hue = 0
	
	local function updateRainbowText()
		hue = hue + 1
		if hue > 360 then
			hue = 0
		end
	
		local color = Color3.fromHSV(hue/360, 1, 1)
		textLabel.TextColor3 = color
	end
	
	while true do
		updateRainbowText()
		wait(0.1)
	end
	
end
coroutine.wrap(PMYMP_script)()
